{
  "extends": "@apify/tsconfig",
  "compilerOptions": {
    "lib": [
      "DOM"
    ],
    "target": "ES2020",
    "module": "Node16",
    "moduleResolution": "Node16",
    "allowJs": true,
    "outDir": "dist",
    "rootDir": ".",
    "skipLibCheck": true,
    "tsBuildInfoFile": "dist/.tsbuildinfo",
    "noImplicitOverride": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./.yarn/**/*"
  ]
}