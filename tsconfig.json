{
  "extends": "@apify/tsconfig",
  "compilerOptions": {
    "lib": [
      "DOM"
    ],
    "module": "ES2022",
    "target": "ES2022",
    "outDir": "dist",
    "rootDir": ".",
    "sourceMap": true,
    "allowImportingTsExtensions": true,
    "emitDeclarationOnly": true
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "./.yarn/**/*"
  ]
}